{
  "name": "Vava Extension",
  "description": "Vava Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:3000/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Vava Event",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
